<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.tvm.doctorcube.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_studyMaterialFragment"
            app:destination="@id/studyMaterialFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <action
            android:id="@+id/action_homeFragment_to_universityFragment"
            app:destination="@id/universityFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit">
            <argument
                android:name="COUNTRY_NAME"
                app:argType="string"
                android:defaultValue="All" />
        </action>
        <action
            android:id="@+id/action_homeFragment_to_universityDetailsFragment"
            app:destination="@id/universityDetailsFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit">
            <argument
                android:name="UNIVERSITY_NAME"
                app:argType="string" />
            <argument
                android:name="COUNTRY"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_homeFragment_to_eventDetailsBottomSheet"
            app:destination="@id/eventDetailsBottomSheet" />
        <action
            android:id="@+id/action_homeFragment_to_fragmentUpcomingEvents"
            app:destination="@id/fragmentUpcomingEvents" />
        <action
            android:id="@+id/action_homeFragment_to_featuresFragment"
            app:destination="@id/featuresFragment" />
    </fragment>
    <fragment
        android:id="@+id/studyMaterialFragment"
        android:name="com.tvm.doctorcube.StudyMaterialFragment"
        android:label="fragment_study_material"
        tools:layout="@layout/fragment_study_material" />
    <fragment
        android:id="@+id/settingsHome"
        android:name="com.tvm.doctorcube.settings.SettingsHome"
        android:label="SettingsHome" >
        <action
            android:id="@+id/action_settingsHome_to_fragmentEditDetails"
            app:destination="@id/fragmentEditDetails" />
        <action
            android:id="@+id/action_settingsHome_to_notificationPref"
            app:destination="@id/notificationPref" />
        <action
            android:id="@+id/action_settingsHome_to_fragmentAbout"
            app:destination="@id/fragmentAbout" />
        <action
            android:id="@+id/action_settingsHome_to_fragmentPrivacy"
            app:destination="@id/fragmentPrivacy" />
        <action
            android:id="@+id/action_settingsHome_to_fragmentFAQ"
            app:destination="@id/fragmentFAQ" />
    </fragment>
    <fragment
        android:id="@+id/universityFragment"
        android:name="com.tvm.doctorcube.university.UniversityFragment"
        android:label="UniversityFragment">
        <argument
            android:name="COUNTRY_NAME"
            app:argType="string"
            android:defaultValue="All" />
        <action
            android:id="@+id/action_universityFragment_to_universityDetailsFragment"
            app:destination="@id/universityDetailsFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit">
            <argument
                android:name="UNIVERSITY_NAME"
                app:argType="string" />
            <argument
                android:name="COUNTRY"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_universityFragment_to_universityDetailsBottomSheet"
            app:destination="@id/universityDetailsBottomSheet" />

    </fragment>
    <fragment
        android:id="@+id/universityDetailsFragment"
        android:name="com.tvm.doctorcube.university.UniversityDetailsFragment"
        android:label="UniversityDetailsFragment">
        <argument
            android:name="UNIVERSITY_NAME"
            app:argType="string" />
        <argument
            android:name="COUNTRY"
            app:argType="string" />
        <action
            android:id="@+id/action_universityDetailsFragment_to_universityDetailsBottomSheet"
            app:destination="@id/universityDetailsBottomSheet" />

    </fragment>
    <fragment
        android:id="@+id/fragmentEditDetails"
        android:name="com.tvm.doctorcube.settings.FragmentEditDetails"
        android:label="FragmentEditDetails" />
    <fragment
        android:id="@+id/notificationPref"
        android:name="com.tvm.doctorcube.settings.NotificationPref"
        android:label="fragment_notification_pref"
        tools:layout="@layout/fragment_notification_pref" />
    <fragment
        android:id="@+id/fragmentAbout"
        android:name="com.tvm.doctorcube.settings.FragmentAbout"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/fragmentPrivacy"
        android:name="com.tvm.doctorcube.settings.FragmentPrivacy"
        android:label="fragment_privacy"
        tools:layout="@layout/fragment_privacy" />
    <fragment
        android:id="@+id/fragmentFAQ"
        android:name="com.tvm.doctorcube.settings.FragmentFAQ"
        android:label="fragment_f_a_q"
        tools:layout="@layout/fragment_f_a_q" />
    <fragment
        android:id="@+id/fragmentUpcomingEvents"
        android:name="com.tvm.doctorcube.home.FragmentUpcomingEvents"
        android:label="FragmentUpcomingEvents" />
    <dialog
        android:id="@+id/eventDetailsBottomSheet"
        android:name="com.tvm.doctorcube.home.EventDetailsBottomSheet"
        android:label="EventDetailsBottomSheet" >
        <action
            android:id="@+id/action_eventDetailsBottomSheet_to_fragmentUpcomingEvents"
            app:destination="@id/fragmentUpcomingEvents" />
    </dialog>
    <fragment
        android:id="@+id/featuresFragment"
        android:name="com.tvm.doctorcube.home.FeaturesFragment"
        android:label="FeaturesFragment" >
        <action
            android:id="@+id/action_featuresFragment_self"
            app:destination="@id/featuresFragment" />
    </fragment>
    <dialog
        android:id="@+id/universityDetailsBottomSheet"
        android:name="com.tvm.doctorcube.university.UniversityDetailsBottomSheet"
        android:label="UniversityDetailsBottomSheet" >
        <argument
            android:name="UNIVERSITY"
            app:argType="com.tvm.doctorcube.university.model.University" />
        <argument
            android:name="universityId"
            app:argType="string"
            app:nullable="true" />
    </dialog>

</navigation>